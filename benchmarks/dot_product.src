var a[3]
var b[3]

log a
for {var i = 0} ; {var cmp = i <> 3; cmp<} ; {i = i + 1} {
  a[i] = [1,5)
}

log b
for {var i = 0} ; {var cmp = i <> 3; cmp<} ; {i = i + 1} {
  b[i] = [1,5)
}

var sum = 0

for {var i = 0} ; {var cmp = i <> 3; cmp<} ; {i = i + 1} {
